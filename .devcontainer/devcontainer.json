{
  "name": "web-dapp",
  "dockerComposeFile": ["../docker-compose.dev.yml", "docker-compose.yml"],
  "service": "web-dapp",
  "workspaceFolder": "/usr/src/app/web-dapp",
  "settings": {
    "workbench.colorCustomizations": {
      "activityBar.background": "#056"
    }
  },
  "extensions": [
    "ms-kubernetes-tools.vscode-kubernetes-tools",
    "mindaro.mindaro"
  ],
  "postCreateCommand": "scripts/post-create-command.sh",
  "features": {
    "kubectl-helm-minikube": {
      "kubectl": "latest",
      "helm": "latest",
    }
  }
}
